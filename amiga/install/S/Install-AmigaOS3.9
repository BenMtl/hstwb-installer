; Install Amiga OS 3.9 Script
; ---------------------------
;
; Date: 2017-07-07
; Author: Henrik Noerfjand Stengaard
;
; An AmigsDOS script to automate installation of Amiga OS 3.9.


; Clear screen
echo "*ec" 


echo "Amiga OS 3.9 Installation"
echo "-------------------------"


; Goto fail, if device OS39: doesn't exist
Assign >NIL: EXISTS OS39:
IF WARN
  echo ""
  echo "*e[1mERROR: Device OS39: doesn't exist!*e[22m"
  SKIP fail
ENDIF


; Goto fail, if device INSTALL: doesn't exist
Assign >NIL: EXISTS INSTALL:
IF WARN
  echo ""
  echo "*e[1mERROR: Device INSTALL: doesn't exist!*e[22m"
  SKIP fail
ENDIF


; Goto fail, if device CD0: doesn't exist
Assign >NIL: EXISTS CD0:
IF WARN
  echo ""
  echo "*e[1mERROR: Device CD0: doesn't exist!*e[22m"
  SKIP fail
ENDIF


; Copy Workbench 3.9
echo ""
echo "Copying Workbench..."

Copy >NIL: CD0:OS-VERSION3.9/Workbench3.5 SYSTEMDIR: ALL
Copy >NIL: CD0:OS-VERSION3.9/Workbench3.9 SYSTEMDIR: ALL

echo "Done"


; Copy libs
echo ""
echo "Copying libs..."

;Copy >NIL: CD0:OS-VERSION3.9/Extras/Libs/mpega.library SYSTEMDIR:Libs
Copy >NIL: CD0:OS-VERSION3.9/Extras/Libs/asyncio.library SYSTEMDIR:Libs

echo "Done"


; Copy backdrops (optional)
echo ""
echo "Copying backdrop..."

Copy >NIL: CD0:OS-VERSION3.9/Extras/Backdrops SYSTEMDIR:Prefs/Presets/Backdrops ALL

echo "Done"


; Copy locale
echo ""
echo "Copying locale..."

Copy >NIL: CD0:OS-VERSION3.9/LOCALE SYSTEMDIR:Locale ALL

echo "Done"


; Copy keymaps
; ------------

echo ""
echo "Copying keymaps..."

Copy >NIL: CD0:OS-VERSION3.9/Workbench3.9/Storage/Keymaps SYSTEMDIR:Devs/Keymaps

echo "Done"


;
; Copy 68040.library, if it doesn't exist
;IF NOT EXISTS "SYSTEMDIR:Libs/68040.library"
;  Copy >NIL: "CD0:OS-VERSION3.9/Extras/Libs/68040.library" SYSTEMDIR:Libs
;ENDIF

; Copy 68060.library, if it doesn't exist
;IF NOT EXISTS "SYSTEMDIR:Libs/68060.library"
;  Copy >NIL: "CD0:OS-VERSION3.9/Extras/Libs/68060.library" SYSTEMDIR:Libs
;ENDIF


; Update FastFileSystem
; ---------------------
IF EXISTS "SYSTEMDIR:L/FastFileSystem"
  Version >NIL: "SYSTEMDIR:L/FastFileSystem" 40 FILE
  IF WARN
    Rename >NIL: "SYSTEMDIR:L/FastFileSystem" "SYSTEMDIR:L/FastFileSystem-old"
  ENDIF 
ENDIF 

Copy >NIL: "CD0:OS-VERSION3.9/L/FastFileSystem" "SYSTEMDIR:L"


; Fix fonts
; ---------

echo ""
echo "Fix fonts..."

Assign >NIL: Fonts: "SYSTEMDIR:Fonts"

SYSTEMDIR:System/FixFonts

Assign >NIL: Fonts: "SYSTEMDIR:Fonts" REMOVE

echo "Done"


; Prefs
; -----

Copy >NIL: "CD0:OS-VERSION3.9/EMERGENCY-DISK/PREFS/ENV-ARCHIVE/SYS/FONT.PREFS" "SYSTEMDIR:Prefs/Env-Archive/Sys"
Copy >NIL: "CD0:EMERGENCY-BOOT/PREFS/ENV-ARCHIVE/SYS/REACTION.PREFS" "SYSTEMDIR:Prefs/Env-Archive/Sys"
Copy >NIL: "CD0:EMERGENCY-BOOT/PREFS/ENV-ARCHIVE/SYS/WBCONFIG.PREFS" "SYSTEMDIR:Prefs/Env-Archive/Sys"
Copy >NIL: "CD0:EMERGENCY-BOOT/PREFS/ENV-ARCHIVE/SYS/WBPATTERN.PREFS" "SYSTEMDIR:Prefs/Env-Archive/Sys"
Copy >NIL: "CD0:EMERGENCY-BOOT/PREFS/ENV-ARCHIVE/SYS/WORKBENCH.PREFS" "SYSTEMDIR:Prefs/Env-Archive/Sys"


; Copy uae prefs, if it exists in install
IF EXISTS INSTALL:Prefs/UAE
  Copy >NIL: INSTALL:Prefs/UAE SYSTEMDIR:Prefs
ENDIF


