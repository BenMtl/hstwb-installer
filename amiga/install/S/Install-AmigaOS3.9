; End, if boing bag 1 doesn't exist
IF NOT EXISTS "OS39:BoingBag39-1.lha"
  SKIP complete
ENDIF

; Create install boing bags prefs
Echo "" >SYS:Prefs/Install-BoingBags

; Copy install boing bags to systemdir
Copy >NIL: SYS:HstWB-Installer/Install-BoingBags SYSTEMDIR: ALL

SKIP complete


; Fail
; ----
LAB fail

echo "Amiga OS 3.9 installation failed."
echo ""
ask "Press ENTER to continue"
SKIP end


; Complete
; --------
LAB complete

echo ""
echo "Amiga OS 3.9 installation is complete."
echo ""
ask "Press ENTER to continue"


; End
; ---
LAB end