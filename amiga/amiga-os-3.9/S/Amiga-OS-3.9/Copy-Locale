.KEY sourcedir/a,targetdir/a
.BRA { 
.KET }  

; Copy Locale Script
; ------------------
;
; Date: 2017-07-07
; Author: Henrik Noerfjand Stengaard
;
; An Amiga script to copy locale.


IF EXISTS "{sourcedir}/bosanski"
  IF EXISTS "{targetdir}/bosanski"
    Copy >NIL: "{sourcedir}/bosanski" "{targetdir}/bosanski" ALL
  ENDIF
ENDIF

IF EXISTS "{sourcedir}/català"
  IF EXISTS "{targetdir}/català"
    Copy >NIL: "{sourcedir}/català" "{targetdir}/català" ALL
  ENDIF
ENDIF

