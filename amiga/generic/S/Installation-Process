; Run Installation
; ----------------
;
; Date: 2017-08-01
; Author: Henrik Noerfjand Stengaard


; Install Workbench, if prefs exists
IF EXISTS "INSTALL:Prefs/Install-Workbench"
  execute "INSTALL:S/Install-Workbench"
ENDIF


; Install Amiga OS 3.9, if prefs exists
IF EXISTS "INSTALL:Prefs/Install-AmigaOS3.9"
  execute "INSTALL:S/Install-AmigaOS3.9"
ENDIF


; Install Kickstart, if prefs exists
IF EXISTS "INSTALL:Prefs/Install-Kickstart"
  execute "INSTALL:S/Install-Kickstart"
ENDIF


; Install Packages, if install boing bags prefs doesn't exist and install packages prefs exists
IF NOT EXISTS INSTALL:Prefs/Install-BoingBags
  IF EXISTS INSTALL:Prefs/Install-Packages
    execute INSTALL:S/Install-Packages
  ENDIF
ENDIF