  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:D.info"
  Copy >NIL: "USERPACKAGEDIR:D.info" "$INSTALLDIR"
ENDIF
echo "Installing 'D'..."

; Extract 'DinoWars' entry archive
IF EXISTS "USERPACKAGEDIR:DinoWars_v1.1.lha"
  lha -q -m1 x "USERPACKAGEDIR:DinoWars_v1.1.lha" "$indexdir/"
ENDIF

