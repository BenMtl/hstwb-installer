; Create 'H' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "H"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:H.info"
  Copy >NIL: "USERPACKAGEDIR:H.info" "$indexdir"
ENDIF
echo "Installing 'H'..."

; Copy 'Hacker' entry directory
IF EXISTS "USERPACKAGEDIR:H\Hacker.info"
  Copy >NIL: "USERPACKAGEDIR:H\Hacker.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:H\Hacker"
  Copy >NIL: "USERPACKAGEDIR:H\Hacker" "$indexdir" ALL
ENDIF

