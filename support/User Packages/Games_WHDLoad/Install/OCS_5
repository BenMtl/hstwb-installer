; Create 'G' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "G"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:G.info"
  Copy >NIL: "USERPACKAGEDIR:G.info" "$indexdir"
ENDIF
echo "Installing 'G'..."

; Copy 'GadgetLostInTime' entry directory
IF EXISTS "USERPACKAGEDIR:G\GadgetLostInTime.info"
  Copy >NIL: "USERPACKAGEDIR:G\GadgetLostInTime.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:G\GadgetLostInTime"
  Copy >NIL: "USERPACKAGEDIR:G\GadgetLostInTime" "$indexdir" ALL
ENDIF

