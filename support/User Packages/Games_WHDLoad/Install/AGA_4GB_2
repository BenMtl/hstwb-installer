; Create 'C' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "C"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:C.info"
  Copy >NIL: "USERPACKAGEDIR:C.info" "$indexdir"
ENDIF
echo "Installing 'C'..."

; Copy 'Cabal' entry directory
IF EXISTS "USERPACKAGEDIR:C\Cabal.info"
  Copy >NIL: "USERPACKAGEDIR:C\Cabal.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:C\Cabal"
  Copy >NIL: "USERPACKAGEDIR:C\Cabal" "$indexdir" ALL
ENDIF

