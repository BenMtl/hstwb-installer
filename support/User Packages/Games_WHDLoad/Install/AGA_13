; Create 'Y' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "Y"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:Y.info"
  Copy >NIL: "USERPACKAGEDIR:Y.info" "$indexdir"
ENDIF
echo "Installing 'Y'..."

