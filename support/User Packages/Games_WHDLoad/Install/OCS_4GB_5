IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:M.info"
  Copy >NIL: "USERPACKAGEDIR:M.info" "$indexdir"
ENDIF
