; Create 'J' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "J"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:J.info"
  Copy >NIL: "USERPACKAGEDIR:J.info" "$indexdir"
ENDIF
echo "Installing 'J'..."

