; Create 'X' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "X"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:X.info"
  Copy >NIL: "USERPACKAGEDIR:X.info" "$indexdir"
ENDIF
echo "Installing 'X'..."

