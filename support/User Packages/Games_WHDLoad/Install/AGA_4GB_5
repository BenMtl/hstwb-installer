; Create 'L' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "L"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:L.info"
  Copy >NIL: "USERPACKAGEDIR:L.info" "$indexdir"
ENDIF
