; Create 'K' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "K"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:K.info"
  Copy >NIL: "USERPACKAGEDIR:K.info" "$indexdir"
ENDIF
echo "Installing 'K'..."

