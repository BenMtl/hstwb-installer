; Create 'S' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "S"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:S.info"
  Copy >NIL: "USERPACKAGEDIR:S.info" "$indexdir"
ENDIF
