; Create 'Q' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "Q"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:Q.info"
  Copy >NIL: "USERPACKAGEDIR:Q.info" "$indexdir"
ENDIF
echo "Installing 'Q'..."

; Copy 'QBic' entry directory
IF EXISTS "USERPACKAGEDIR:Q\QBic.info"
  Copy >NIL: "USERPACKAGEDIR:Q\QBic.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:Q\QBic"
  Copy >NIL: "USERPACKAGEDIR:Q\QBic" "$indexdir" ALL
ENDIF

; Copy 'Qix' entry directory
