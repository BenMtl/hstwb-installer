; Create 'R' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "R"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:R.info"
  Copy >NIL: "USERPACKAGEDIR:R.info" "$indexdir"
ENDIF
echo "Installing 'R'..."

