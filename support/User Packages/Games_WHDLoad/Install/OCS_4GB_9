  Copy >NIL: "USERPACKAGEDIR:W\WildWestWorld.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W\WildWestWorld"
  Copy >NIL: "USERPACKAGEDIR:W\WildWestWorld" "$indexdir" ALL
ENDIF

