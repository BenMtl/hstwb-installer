; EAB WHDLoad Pack Install Script
; -------------------------------
;
; Author: Henrik Noerfjand Stengaard
; Date: 2017-10-14

; 'Games_WHDLoad_AGA' install script

; Install '0-9'
IF EXISTS "USERPACKAGEDIR:0-9"
  echo "Copying '0-9'..."
  IF NOT EXISTS "$INSTALLDIR/0-9"
    MakePath >NIL: "$INSTALLDIR/0-9"
  ENDIF
  Copy >NIL: "USERPACKAGEDIR:0-9" "$INSTALLDIR/0-9" ALL
  IF EXISTS "USERPACKAGEDIR:0-9.info"
    Copy >NIL: "USERPACKAGEDIR:0-9.info" "$INSTALLDIR"
  ENDIF
ENDIF

; Install 'A'
IF EXISTS "USERPACKAGEDIR:A"
  echo "Copying 'A'..."
  IF NOT EXISTS "$INSTALLDIR/A"
    MakePath >NIL: "$INSTALLDIR/A"
