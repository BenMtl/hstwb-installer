; Create 'P' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "P"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:P.info"
  Copy >NIL: "USERPACKAGEDIR:P.info" "$indexdir"
ENDIF
echo "Installing 'P'..."

