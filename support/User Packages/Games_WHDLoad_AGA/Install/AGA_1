; Create '0-9' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "0-9"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9.info"
  Copy >NIL: "USERPACKAGEDIR:0-9.info" "$indexdir"
ENDIF
echo "Installing '0-9'..."

; Copy '1869AGA' entry directory
IF EXISTS "USERPACKAGEDIR:0-9\1869AGA.info"
  Copy >NIL: "USERPACKAGEDIR:0-9\1869AGA.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9\1869AGA"
  Copy >NIL: "USERPACKAGEDIR:0-9\1869AGA" "$indexdir" ALL
ENDIF

; Copy '1869AGADe' entry directory
IF EXISTS "USERPACKAGEDIR:0-9\1869AGADe.info"
  Copy >NIL: "USERPACKAGEDIR:0-9\1869AGADe.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9\1869AGADe"
  Copy >NIL: "USERPACKAGEDIR:0-9\1869AGADe" "$indexdir" ALL
ENDIF

