  Copy >NIL: "USERPACKAGEDIR:O\OgAGA.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:O\OgAGA"
  Copy >NIL: "USERPACKAGEDIR:O\OgAGA" "$indexdir" ALL
ENDIF

