ENDIF

; Create 'U' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "U"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:U.info"
  Copy >NIL: "USERPACKAGEDIR:U.info" "$indexdir"
ENDIF
echo "Installing 'U'..."

