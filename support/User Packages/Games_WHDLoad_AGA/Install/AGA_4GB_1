  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I.info"
  Copy >NIL: "USERPACKAGEDIR:I.info" "$indexdir"
ENDIF
echo "Installing 'I'..."

; Copy 'ImpossibleMission2025AGA' entry directory
IF EXISTS "USERPACKAGEDIR:I\ImpossibleMission2025AGA.info"
  Copy >NIL: "USERPACKAGEDIR:I\ImpossibleMission2025AGA.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:I\ImpossibleMission2025AGA"
  Copy >NIL: "USERPACKAGEDIR:I\ImpossibleMission2025AGA" "$indexdir" ALL
ENDIF

