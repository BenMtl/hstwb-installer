  Copy >NIL: "USERPACKAGEDIR:W\WorldsWave" "$indexdir" ALL
ENDIF

; Copy 'WorldsWave2' entry directory
IF EXISTS "USERPACKAGEDIR:W\WorldsWave2.info"
  Copy >NIL: "USERPACKAGEDIR:W\WorldsWave2.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W\WorldsWave2"
  Copy >NIL: "USERPACKAGEDIR:W\WorldsWave2" "$indexdir" ALL
ENDIF

; Copy 'WorldsWave3' entry directory
IF EXISTS "USERPACKAGEDIR:W\WorldsWave3.info"
  Copy >NIL: "USERPACKAGEDIR:W\WorldsWave3.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W\WorldsWave3"
  Copy >NIL: "USERPACKAGEDIR:W\WorldsWave3" "$indexdir" ALL
ENDIF

; Copy 'WorldsWave4' entry directory
IF EXISTS "USERPACKAGEDIR:W\WorldsWave4.info"
  Copy >NIL: "USERPACKAGEDIR:W\WorldsWave4.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:W\WorldsWave4"
  Copy >NIL: "USERPACKAGEDIR:W\WorldsWave4" "$indexdir" ALL
ENDIF

; Create 'X' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "X"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:X.info"
  Copy >NIL: "USERPACKAGEDIR:X.info" "$indexdir"
ENDIF
echo "Installing 'X'..."

; Copy 'Xenon2MusicRip' entry directory
IF EXISTS "USERPACKAGEDIR:X\Xenon2MusicRip.info"
  Copy >NIL: "USERPACKAGEDIR:X\Xenon2MusicRip.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:X\Xenon2MusicRip"
  Copy >NIL: "USERPACKAGEDIR:X\Xenon2MusicRip" "$indexdir" ALL
ENDIF

; Copy 'XPose' entry directory
IF EXISTS "USERPACKAGEDIR:X\XPose.info"
  Copy >NIL: "USERPACKAGEDIR:X\XPose.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:X\XPose"
  Copy >NIL: "USERPACKAGEDIR:X\XPose" "$indexdir" ALL
ENDIF

; Create 'Z' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "Z"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:Z.info"
  Copy >NIL: "USERPACKAGEDIR:Z.info" "$indexdir"
ENDIF
echo "Installing 'Z'..."

; Copy 'ZillionDreams' entry directory
IF EXISTS "USERPACKAGEDIR:Z\ZillionDreams.info"
  Copy >NIL: "USERPACKAGEDIR:Z\ZillionDreams.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:Z\ZillionDreams"
  Copy >NIL: "USERPACKAGEDIR:Z\ZillionDreams" "$indexdir" ALL
ENDIF
