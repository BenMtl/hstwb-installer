; Create 'F' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "F"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:F.info"
  Copy >NIL: "USERPACKAGEDIR:F.info" "$indexdir"
ENDIF
echo "Installing 'F'..."

