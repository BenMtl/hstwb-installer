; Create '0-9' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "0-9"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9.info"
  Copy >NIL: "USERPACKAGEDIR:0-9.info" "$indexdir"
ENDIF
echo "Installing '0-9'..."

; Copy '1001StolenIdeas' entry directory
IF EXISTS "USERPACKAGEDIR:0-9\1001StolenIdeas.info"
  Copy >NIL: "USERPACKAGEDIR:0-9\1001StolenIdeas.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9\1001StolenIdeas"
  Copy >NIL: "USERPACKAGEDIR:0-9\1001StolenIdeas" "$indexdir" ALL
ENDIF

; Copy '100MostRememberedC64Tunes' entry directory
IF EXISTS "USERPACKAGEDIR:0-9\100MostRememberedC64Tunes.info"
  Copy >NIL: "USERPACKAGEDIR:0-9\100MostRememberedC64Tunes.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9\100MostRememberedC64Tunes"
  Copy >NIL: "USERPACKAGEDIR:0-9\100MostRememberedC64Tunes" "$indexdir" ALL
ENDIF

; Copy '242' entry directory
IF EXISTS "USERPACKAGEDIR:0-9\242.info"
  Copy >NIL: "USERPACKAGEDIR:0-9\242.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:0-9\242"
  Copy >NIL: "USERPACKAGEDIR:0-9\242" "$indexdir" ALL
ENDIF

