; Create 'T' index directory
set indexdir "`execute INSTALLDIR:S/CombinePath "$INSTALLDIR" "T"`"
IF NOT EXISTS "$indexdir"
  MakePath >NIL: "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:T.info"
  Copy >NIL: "USERPACKAGEDIR:T.info" "$indexdir"
ENDIF
echo "Installing 'T'..."

