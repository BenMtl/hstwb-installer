; Copy 'FutureSynthetix' entry directory
IF EXISTS "USERPACKAGEDIR:F\FutureSynthetix.info"
  Copy >NIL: "USERPACKAGEDIR:F\FutureSynthetix.info" "$indexdir"
ENDIF
IF EXISTS "USERPACKAGEDIR:F\FutureSynthetix"
  Copy >NIL: "USERPACKAGEDIR:F\FutureSynthetix" "$indexdir" ALL
ENDIF

